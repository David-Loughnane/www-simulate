{
  "name": "simulate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "browserify -t [ babelify --presets [ es2015 ] ] src/app.js > static/bundle.js",
    "build:w": "watchify -t [ babelify --presets [ es2015 ] ] src/app.js -o static/bundle.js -v",
    "build:wb": "npm run build:w &",
    "dev": "npm run build:wb && npm run serve",
    "serve": "live-server .",
    "test": "tape test/*.test.js",
    "cover": "istanbul cover tape test/*.test.js && istanbul check-coverage --functions 90 --branches 90 --statements 90 --lines 90"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "catch-links": "^2.0.0",
    "js-network-vis": "0.0.4",
    "routes": "^2.1.0",
    "yo-yo": "^1.1.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "istanbul": "^0.4.2",
    "jsdom": "^8.3.0",
    "live-server": "^0.9.2",
    "tape": "^4.5.1",
    "watchify": "^3.7.0"
  }
}
